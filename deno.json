{
  "name": "@tijs/atproto-oauth-hono",
  "version": "0.2.1",
  "description": "Complete ATProto OAuth integration for Hono applications on Val.Town",
  "license": "MIT",
  "exports": {
    ".": "./mod.ts",
    "./drizzle": "./drizzle.ts"
  },
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": false,
    "lib": ["dom", "dom.iterable", "deno.ns"]
  },
  "lint": {
    "rules": {
      "exclude": [
        "no-explicit-any"
      ],
      "include": [
        "no-slow-types"
      ]
    }
  },
  "tasks": {
    "test": "deno test --allow-all",
    "fmt-check": "deno fmt --check",
    "quality": "deno fmt && deno lint && deno test --allow-all",
    "ci": "deno fmt --check && deno lint && deno test --allow-all"
  },
  "imports": {
    "@hono/hono": "jsr:@hono/hono@4.9.6",
    "@tijs/oauth-client-deno": "jsr:@tijs/oauth-client-deno@1.0.5",
    "@tijs/hono-oauth-sessions": "jsr:@tijs/hono-oauth-sessions@0.2.3",
    "@std/assert": "jsr:@std/assert@1",
    "drizzle-orm": "npm:drizzle-orm@0.33.0",
    "drizzle-orm/sqlite-core": "npm:drizzle-orm@0.33.0/sqlite-core"
  }
}
